{
    "version": 2,
    "builds": [
        {
            "src": "discussion_forum/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "50mb" , "runtime" : "python3.9" }
        },
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static-build",
            "config" :{
                "distDir" : "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src" : "/sttic/(.*)",
            "dest": "cussion_forum/wsgi.py"
        },
        {

            "src": "/(.*)",
            "dest": "discussion_forum/wsgi.py"
        }
    ]
}
